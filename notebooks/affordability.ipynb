   "execution_count": 201,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Calculate Median Rent (R) as rent as a percentage of median household income using 'extracted_price'\n",
    "sa2_grouped['Median_Rent'] = sa2_grouped['extracted_price'] / sa2_grouped['Median_Income']\n",
    "# availability of housing: Ratio of available rentals to the population in each SA2\n",
    "sa2_grouped['availability'] = sa2_grouped['property_count'] / sa2_grouped['Population']"
   ]
